<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.umangpatel.smarthealth">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".TopLevelActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BrowseDiseaseCategoryActivity"
            android:label="@string/title_activity_browse_disease_category"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DiseaseActivity"
            android:label="@string/title_activity_disease"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SearchBySymptomsCategoryActivity"
            android:label="@string/title_activity_search_by_symptoms_category"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ShowVideoCategoryActivity"
            android:label="@string/title_activity_show_video_category"
            android:theme="@style/AppTheme.NoActionBar"/>
    </application>

</manifest>
